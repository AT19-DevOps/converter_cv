            "tags": [
              "Download"
            ],
            "summary": "Get converted file",
            "description": "Download converted file",
            "operationId": "get.download",
            "parameters": [

              {
                "in": "query",
                "name": "file_name",
                "schema": {
                  "type": "string",
                  "example": "file"
                },
                "description": "Please insert the file you want to download:"
              }
            ],
            "responses": {
              "200": {
                "description": "Successful operation",
                "content": {
                  "image/png": {
                    "schema": {
                      "type": "string",
                      "format": "binary"
                    }
                  }
                }
              }
            },
            "400": {
              "description": "Invalid operation"
            }
                }
      },
    "/videotoimagee/zip": {
      "post": {
        "tags": [
          "Video to Images"
        ],
        "summary": "Get images from a video in a zip file",
        "description": "Convert inserted video to a given image type",
        "operationId": "post.videotoimages",
        "requestBody": {
          "description": "Please insert the video to convert:",
          "content": {

            "multipart/form-data": {

              "schema": {
                "type": "object",
                "properties": {
                  "input_file": {
                    "type": "string",
                    "format": "binary"
                  }

                }
              }

            }
          },
          "required": true
        },
        "parameters": [

          {
            "in": "query",
            "name": "output_file",
            "schema": {
              "type": "string",
              "example": ".png"
            },
            "description": "Please insert the output type of the image that you prefer:"
          },
          {
            "in": "query",
            "name": "fps",
            "schema": {
              "type": "string",
              "example": "1"
            },
            "description": "Please insert the fps of the inserted video:"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "image/png": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          },
          "400": {
            "description": "Invalid video supplied"
          }
        }
      }
      },
